<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <template id="rksv_assets_css" name="pos_rksv_css" inherit_id="point_of_sale.index">
            <xpath expr="//link[@id='pos-stylesheet']" position="after">
                <link href="/pos_rksv/static/src/css/rksv.css" rel="stylesheet"/>
                <link href="/pos_rksv/static/src/css/rksv8.css" rel="stylesheet"/>
            </xpath>
        </template>

        <template id="rksv_assets" inherit_id="point_of_sale.assets_backend">
          <xpath expr="." position="inside">
              <script type="text/javascript" src="/pos_rksv/static/src/js/compat.js"></script>
              <script type="text/javascript" src="/pos_rksv/static/src/js/models.js"></script>
              <script type="text/javascript" src="/pos_rksv/static/src/js/pos.js"></script>
              <script type="text/javascript" src="/pos_rksv/static/src/js/rksv.js"></script>
              <script type="text/javascript" src="/pos_rksv/static/src/js/chrome.js"></script>
              <script type="text/javascript" src="/pos_rksv/static/src/js/popups.js"></script>
              <script type="text/javascript" src="/pos_rksv/static/src/js/screens.js"></script>
              <script type="text/javascript" src="/pos_rksv/static/src/js/devices.js"></script>
              <script type="text/javascript" src="/pos_rksv/static/src/js/db.js"></script>
          </xpath>
        </template>

    </data>
</openerp>